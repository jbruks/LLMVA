import nmap

def tcp_syn_scan(target='192.168.1.0/24', ports='1-1024'):
    # Initialize the Nmap PortScanner
    scanner = nmap.PortScanner()
    print(f"Initiating TCP SYN Scan on {target} for ports {ports}...")

